{
  "manifest_version": 3,
  "name": "Dark filter",
  "version": "1337.0",
  "description": "кароч простенький фильтр затемнения",

  "icons": {
    "16": "icon-16.png",
    "32": "icon-32.png",
    "48": "icon-48.png",
    "128": "icon-128.png"
  },

  "permissions": ["activeTab", "storage"],

  "action": {
    "default_popup": "popup.html"
  },

  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
}